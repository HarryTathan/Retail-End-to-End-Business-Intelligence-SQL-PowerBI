  <DataSets>
    <DataSet Name="LowStockData">
      <Query>
        <DataSourceName>DataSource3</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StoreName">
            <Value>=Parameters!StoreName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Threshold">
            <Value>=Parameters!Threshold.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    d_store.store_name,
    d_prod.category_name,
    d_prod.product_name,
    d_prod.sku,
    SUM(f_inv.quantity_change) AS current_stock,
    CASE 
        WHEN SUM(f_inv.quantity_change) &lt;= 0 THEN 'Critical - Out of Stock'
        WHEN SUM(f_inv.quantity_change) &lt;= 100 THEN 'Urgent Reorder'
        ELSE 'Low Stock' 
    END AS stock_status
FROM 
    gold.fct_inventory f_inv
INNER JOIN 
    gold.dim_product d_prod ON f_inv.product_key = d_prod.product_key
INNER JOIN 
    gold.dim_store d_store ON f_inv.store_key = d_store.store_key
WHERE 
    d_store.store_name = @StoreName 
GROUP BY 
    d_store.store_name,
    d_prod.category_name,
    d_prod.product_name,
    d_prod.sku
HAVING 
    SUM(f_inv.quantity_change) &lt; @Threshold
ORDER BY 
    current_stock ASC;</CommandText>
      </Query>
      <Fields>
        <Field Name="store_name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>store_name</DataField>
